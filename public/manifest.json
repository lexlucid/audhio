{
    "manifest_version": 3,
    "name": "audhio",
    "version": "1.0",
    "description": "Text-to-speech extension using ElevenLabs API",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "css": ["content.css"]
        }
    ],
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": ["<all_urls>"]
}